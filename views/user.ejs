<h1>Welcome back <%= username.toUpperCase() %> </h1>

<h2>here are the latest posts:</h2>

<% for (var i=0; i<posts.length; i++){ %>
    <li>
      <%=posts[i].post%> - <strong><%=posts[i].name%></strong>
    </li> 
<%}%>
<!--testing foreach and es6-->
<% posts.forEach((post)=>{ %>
    <li>
        <%=post.post%> - <strong><%=post.name%></strong>
    </li>
<%})%>

